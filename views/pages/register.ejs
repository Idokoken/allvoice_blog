  <div class="contains">
    <div class="main">
      <div class="m-1 d-flex flex-row align-items-center">
        <img
          src="/images/brand.png"
          alt="brand"
          width="30"
          height="30"
          class="d-inline-block align-text-top me-1"
        />
        <h3 class="text-dark text-center">AllVoice</h3>
      </div>
      <h1 class="mt-3 text-center">Create Account</h1>
      <% if (typeof errors == 'object' && errors) for( let i = 0; i <
      errors.length; i++ ) { %>
      < class="alert alert-danger"><%= errors[i].msg %></div>
      <% } %> <% if (typeof error == 'string') { %>
      <div class="alert alert-danger"><%= error %></div>
      <% } %>
      
      <form action="/auth/register" method="post">
        <div class="mb-3">
          <label for="firstname" class="form-label">Username</label>
          <input type="text" class="form-control" name="username" />
        </div>
        <div class="mb-3">
          <label for="Email" class="form-label">Email</label>
          <input type="email" class="form-control" name="email" />
        </div>
        <div class="mb-3">
          <label for="password" class="form-label">Password</label>
          <input type="password" class="form-control" name="password" />
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
      <div class="mt-3">
        <p class="">
          Already have an acoount? <a href="/auth/login">Login</a>
        </p>
      </div>
    </div>
  </div>
      

   
